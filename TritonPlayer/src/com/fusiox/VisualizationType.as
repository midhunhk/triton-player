package com.fusiox
{
/**
 *	The VisualizationType defines constants for the Visualizations
 * 	that are used in the application
 */
public class VisualizationType
{
	/**
	 * Specifies the Waves Visualization
	 */
	public static const WAVE:String = "wave";
	
	/**
	 * Specifies no Visualization
	 */
	public static const NONE:String = "";
	
	/**
	 * Specifies the bars Visualization
	 */
	public static const BARS:String = "bars";
	
	/**
	 * Specifies the line Visualization
	 */
	public static const LINE:String = "line";	

}
}